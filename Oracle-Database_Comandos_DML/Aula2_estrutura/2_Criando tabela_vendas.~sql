CREATE TABLE TINA_TB_VENDAS
(
NUMERO VARCHAR(5) NOT NULL,
DATA_VENDA DATE NULL,
CPF VARCHAR(11) NOT NULL,
MATRICULA VARCHAR(5) NOT NULL, 
IMPOSTO FLOAT NULL,
PRIMARY KEY (NUMERO)
);

ALTER TABLE TINA_TB_VENDAS
ADD CPF NVARCHAR2(11) not null

ALTER TABLE TINA_TB_VENDAS
ADD CONSTRAINT FK_TINA_TB_CLIENTES
FOREIGN KEY (CPF) 
REFERENCES TINA_TB_CLIENTES(CPF);


ALTER TABLE TINA_TB_VENDAS
ADD MATRICULA NVARCHAR2(5) not null;



ALTER TABLE TINA_TB_VENDAS
ADD CONSTRAINT FK_TINA_TB_VENDEDORES
FOREIGN KEY (MATRICULA) 
REFERENCES TINA_TB_VENDEDORES(MATRICULA);
